# AWS n8n Environment Variables
# Copy this file and set values in AWS App Runner / ECS console

# Server Configuration
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_LISTEN_ADDRESS=0.0.0.0

# n8n Recommended Settings
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
N8N_RUNNERS_ENABLED=true
GENERIC_TIMEZONE=Asia/Manila
TZ=Asia/Manila

# Authentication (set your own)
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=your_username_here
N8N_BASIC_AUTH_PASSWORD=your_strong_password_here

# Service URLs (set after deployment - get from App Runner URL)
N8N_HOST=https://your-app-runner-url.awsapprunner.com
WEBHOOK_URL=https://your-app-runner-url.awsapprunner.com/

# Database Configuration (from AWS RDS)
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=your-rds-endpoint.region.rds.amazonaws.com
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8nuser
DB_POSTGRESDB_PASSWORD=your_rds_password_here
DB_POSTGRESDB_SSLMODE=require
DB_POSTGRESDB_SSL=true
DB_POSTGRESDB_SSL_REJECT_UNAUTHORIZED=false

